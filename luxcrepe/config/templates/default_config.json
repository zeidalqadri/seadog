{
  "seadog_integration": {
    "intelligence_enabled": true,
    "real_time_monitoring": true,
    "auto_recovery": true,
    "test_suite_type": "RECONNAISSANCE",
    "output_directory": "./seadog_test_results",
    "parallel_execution": true,
    "timeout_minutes": 30
  },
  "scraper_config": {
    "delay": 1.0,
    "max_retries": 3,
    "retry_delay": 2.0,
    "user_agent": "Mozilla/5.0 (compatible; LuxcrepeScraper/1.0)",
    "headers": {
      "Accept": "text/html,application/xhtml+xml,application/xml;q=0.9,*/*;q=0.8",
      "Accept-Language": "en-US,en;q=0.5",
      "Accept-Encoding": "gzip, deflate",
      "Connection": "keep-alive"
    }
  },
  "test_scenarios": {
    "reconnaissance": {
      "depth": "COMPREHENSIVE",
      "opsec_level": "COVERT",
      "intelligence_requirements": [
        "TARGET_DISCOVERY",
        "INFRASTRUCTURE_MAPPING", 
        "INTELLIGENCE_GATHERING",
        "SURVEILLANCE_MONITORING"
      ]
    },
    "performance_thresholds": {
      "max_response_time": 10.0,
      "min_success_rate": 0.85,
      "max_error_rate": 0.15
    }
  },
  "intelligence_system": {
    "monitoring_level": "ACTIVE",
    "alert_thresholds": {
      "response_time_warning": 5.0,
      "response_time_critical": 10.0,
      "success_rate_warning": 0.85,
      "success_rate_critical": 0.7,
      "error_rate_warning": 0.1,
      "error_rate_critical": 0.2
    },
    "analysis_interval": 60,
    "data_retention_hours": 24
  }
}